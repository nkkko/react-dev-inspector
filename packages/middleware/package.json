{
  "name": "@react-dev-inspector/middleware",
  "version": "2.0.0-beta.3",
  "sideEffects": false,
  "description": "express middleware for react-dev-inspector to launch local IDE",
  "main": "lib",
  "module": "es",
  "types": "es",
  "scripts": {
    "clean": "rimraf coverage dist es lib build",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm-run-all clean --parallel build:esm build:cjs"
  },
  "repository": "https://github.com/zthxxx/react-dev-inspector/tree/master/packages/middleware",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "author": "zthxxx <zthxxx.me@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zthxxx/react-dev-inspector/issues"
  },
  "homepage": "https://github.com/zthxxx/react-dev-inspector",
  "files": [
    "README.md",
    "src",
    "es",
    "lib"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "react-dev-utils": "12.0.1"
  },
  "devDependencies": {
    "@types/connect": "3.4.35",
    "@types/express": "4.17.13",
    "@types/node": "16.18.7",
    "@types/react-dev-utils": "9.0.11",
    "@zthxxx/tsconfig": "1.0.2",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "typescript": "5.1.6"
  }
}
